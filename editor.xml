<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bchanged\b:.*">
      <option name="FOREGROUND" value="7dc0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="\beventcaller\b.*" />
    <filter name="Changed">
      <pattern index="2" />
      <pattern index="3" />
    </filter>
    <filter name="Events">
      <pattern index="3" />
    </filter>
    <filter name="TODO ONLY">
      <pattern index="1" />
      <pattern index="0" />
    </filter>
  </component>
</application>